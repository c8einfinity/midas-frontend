<?xml version="1.0"?>
<!-- /**
 * acl.xml
 */ -->
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Motus_Quotesystem::quotesystem" title="Motus Quote System" sortOrder="10" >
                    <resource id="Motus_Quotesystem::quotes" title="Manage Quotes" sortOrder="10">
                    </resource>
                </resource>
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Motus_Quotesystem::config_quotesystem" title="Quotesystem Configuration section"/>
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>